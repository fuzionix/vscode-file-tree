{
  "name": "file-tree-extractor",
  "displayName": "File Tree Extractor",
  "publisher": "Fuzionix",
  "description": "Generate a directory structure for the current project by simply copy and paste",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "file-tree-extractor.copyFileTree",
        "title": "Copy File Tree"
      },
      {
        "command": "file-tree-extractor.copyFileTreeFromThisDir",
        "title": "Copy File Tree From This Directory"
      }
  ],
    "menus": {
      "explorer/context": [
        {
          "command": "file-tree-extractor.copyFileTree",
          "group": "6_copypath@500"
        },
        {
          "command": "file-tree-extractor.copyFileTreeFromThisDir",
          "group": "6_copypath@500"
        }
      ]
    },
    "keybindings": [
    {
      "command": "file-tree-extractor.copyFileTree",
      "key": "ctrl+alt+c"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
